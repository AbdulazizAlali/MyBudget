<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="project.mybudget">



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_wallet"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_wallet_round"
        android:supportsRtl="true"
        android:configChanges="layoutDirection|locale"
        android:theme="@style/AppTheme">
        <activity android:name=".GeneratedReportActivity"
            android:theme="@style/AppTheme.ActionBar"
            android:label="Report"/>

        <activity
            android:name=".Admin.AdminDashboard"
            android:theme="@style/AppTheme.ActionBar" />
        <activity
            android:name=".UsersActivity"
            android:theme="@style/AppTheme.ActionBar" />
        <activity
            android:name=".CategoriesActivity"
            android:label= "@string/categories"
            android:theme="@style/AppTheme.ActionBar" />
        <activity
            android:name=".ReportsActivity"
            android:label="Reports" />
        <activity android:name=".SettingsActivity" />
        <activity
            android:name=".Admin.WalletsActivity"
            android:theme="@style/AppTheme.ActionBar" />
        <activity android:name=".Authentication.Register" />
        <activity
            android:name=".Admin.ReceiptsActivity"
            android:theme="@style/AppTheme.ActionBar" />


        <activity
            android:name=".Authentication.Login"
            android:label="Register" />
        <activity
            android:name=".MainActivity"
            android:label="Main"
            android:theme="@style/AppTheme"/>
        <activity
            android:name=".StartupActivity"
            android:gravity="center"
            android:label="MY Budget"
            android:textColor="#ffffffff">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>